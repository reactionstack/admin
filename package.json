          }
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          [
            "@babel/preset-react"
          ]
        ],
        "plugins": [
          "rewire-exports",
          "babel-plugin-inline-import",
          [
            "@babel/plugin-proposal-decorators",
