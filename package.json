            {
              "loose": false
            }
          ],
          "@babel/plugin-proposal-json-strings",
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "@reactioncommerce/api-utils": "./src/imports/utils",
                "@reactioncommerce/reaction-collections": "./src/imports/plugins/core/collections",
                "@reactioncommerce/reaction-components": "./src/imports/plugins/core/components/lib",
                "@reactioncommerce/reaction-router": "./src/imports/plugins/core/router/lib",
                "@reactioncommerce/reaction-ui": "./src/imports/plugins/core/ui/client/components",
                "underscore": "lodash"
              }
            }
          ]
        ]
      }
